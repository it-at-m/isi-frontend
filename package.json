{
  "name": "lint-config",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "check": "npx prettier \"**/*\" --check --ignore-unknown",
    "lint": "npx prettier \"**/*\" --write --ignore-unknown"
  },
  "devDependencies": {
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-java": "2.3.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
