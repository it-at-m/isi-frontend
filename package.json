{
  "name": "lint-config",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "check": "npx prettier --check --ignore-unknown .",
    "lint": "npx prettier --write --ignore-unknown ."
  },
  "devDependencies": {
    "husky": "^8.0.2",
    "lint-staged": "^13.2.0",
    "prettier": "2.8.4",
    "prettier-plugin-java": "^2.1.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
