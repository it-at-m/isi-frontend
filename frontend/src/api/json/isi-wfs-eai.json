  "components": {
    "schemas": {
      "CoordinateDto": {
        "required": [
          "lat",
          "lon"
        ],
        "type": "object",
        "properties": {
          "lat": {
            "type": "number",
            "description": "Die geographische Breite im Standard EPSG:4326 (WGS84)"
          },
          "lon": {
            "type": "number",
            "description": "Die geographische Länge im Standard EPSG:4326 (WGS84)"
          }
        },
        "description": "Die Koordinate im Standard EPSG:4326 (WGS84)"
      },
      "CoordinatesDto": {
        "required": [
          "coordinates"
        ],
        "type": "object",
        "properties": {
          "coordinates": {
            "maxItems": 2147483647,
            "minItems": 3,
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CoordinateDto"
            }
          }
        },
        "description": "Die Koordinatenliste im Standard EPSG:4326 (WGS84)"
      },
      "InformationResponseDto": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "ERROR",
              "WARNING",
              "INFORMATION_SUCCESS",
              "INFORMATION_NEUTRAL"
            ]
          },
          "timestamp": {
            "type": "string",
            "format": "date-time"
          },
          "traceId": {
            "type": "string"
          },
          "spanId": {
            "type": "string"
          },
          "httpStatus": {
            "type": "integer",
            "format": "int32"
          },
          "originalException": {
            "type": "string"
          },
          "messages": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
